<?xml version="1.0" encoding="utf-8"?>
<!--
    Holds empty _future and _past plurals so they can be overridden
    in languages that need it (like German).
-->
<resources>
    <plurals name="seconds">
        <item quantity="one">second</item>
        <item quantity="other">seconds</item>
    </plurals>
    <plurals name="seconds_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="seconds_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="minutes">
        <item quantity="one">minute</item>
        <item quantity="other">minutes</item>
    </plurals>
    <plurals name="minutes_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="minutes_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="hours">
        <item quantity="one">hour</item>
        <item quantity="other">hours</item>
    </plurals>
    <plurals name="hours_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="hours_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="days">
        <item quantity="one">day</item>
        <item quantity="other">days</item>
    </plurals>
    <plurals name="days_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="days_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="weeks">
        <item quantity="one">week</item>
        <item quantity="other">weeks</item>
    </plurals>
    <plurals name="weeks_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="weeks_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="months">
        <item quantity="one">month</item>
        <item quantity="other">months</item>
    </plurals>
    <plurals name="months_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="months_past">
        <item quantity="other"/>
    </plurals>

    <plurals name="years">
        <item quantity="one">year</item>
        <item quantity="other">years</item>
    </plurals>
    <plurals name="years_future">
        <item quantity="other"/>
    </plurals>
    <plurals name="years_past">
        <item quantity="other"/>
    </plurals>

    <string name="time_ago">${time} ago</string>
    <string name="time_in_future">in ${time}</string>
    <string name="now">now</string>
</resources>